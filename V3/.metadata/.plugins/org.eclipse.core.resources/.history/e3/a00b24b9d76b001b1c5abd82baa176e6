package coreGame;

public class Controller{
	Model MODEL = new Model();
	Viewer VIEW = new Viewer();
	
	Controller(){
		
	}
	public void Start() {
		VIEW.SS.setOpen(true);
		VIEW.SS.createStartScreen();
	}
	//gets
	public void getMainMenuScreen() {
		VIEW.MM.setResolution(MODEL.getHeight(), MODEL.getWidth());
	}
	//sets


}
